[MASTER]
ignore=__pycache__,data/MNIST

[MESSAGES CONTROL]
disable=missing-docstring,invalid-name,too-few-public-methods,too-many-positional-arguments

[FORMAT]
max-line-length=120

[DESIGN]
max-args=10
max-locals=20
max-branches=12
max-statements=50
max-parents=7
max-attributes=10
max-public-methods=20
max-returns=6
max-bool-expr=5
max-nested-blocks=5
max-module-lines=1000

[REPORTS]
output-format=colorized
reports=no
